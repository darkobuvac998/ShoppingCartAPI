
node {
    stage('Run parallel') {
      parallel
        Administration: {buildStageDynamically('Admin').call()},
        TMF: {buildStageDynamically('TMF').call()}
    }
}

def buildStageDynamically(stageName) {
  return {
    stage("${stageName}") {
      node {
        label 'dotnet-agent'
      }

      sh 'echo Hello'
    }
  }
}

